{
    "name": "deal-backend",
    "version": "0.0.1",
    "description": "testing Login version",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "NODE_ENV=prod nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "NODE_ENV=prod nest start",
        "dev": "NODE_ENV=dev nest start --watch",
        "start:dev": "NODE_ENV=dev nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "NODE_ENV=prod node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^1.2.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mapped-types": "^1.0.1",
        "@nestjs/mongoose": "^9.0.2",
        "@nestjs/passport": "^8.1.0",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/platform-socket.io": "^8.4.4",
        "@nestjs/schedule": "^1.0.2",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^5.2.0",
        "@nestjs/testing": "^8.0.0",
        "@nestjs/websockets": "^8.4.4",
        "@types/express": "^4.17.13",
        "@types/jest": "27.0.2",
        "@types/lodash": "^4.14.178",
        "@types/mongoose-delete": "^0.5.8",
        "@types/node": "^16.0.0",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "axios": "^0.26.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "collect.js": "^4.31.3",
        "crypto": "^1.0.1",
        "email-verify": "^0.2.1",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "express-fileupload": "^1.3.1",
        "fs": "^0.0.1-security",
        "geolib": "^3.3.3",
        "jest": "^27.2.5",
        "lodash": "^4.17.21",
        "minio": "^7.0.26",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.34",
        "mongoose": "^6.2.7",
        "mongoose-delete": "^0.5.4",
        "mongoose-paginate-v2": "^1.6.3",
        "mongoose-unique-validator": "^3.0.0",
        "multer": "^1.4.4",
        "nestjs-minio-client": "^1.1.0",
        "nestjs-pdf": "^1.0.4",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pdf-creator-node": "^2.3.4",
        "prettier": "^2.3.2",
